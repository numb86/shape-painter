{
  "name": "shape-painter",
  "author": "numb86",
  "description": "It is an application that you to easily draw tree diagrams etc. using a browser.",
  "keywords": [
    "shape",
    "tree",
    "binary tree"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=12.14.1",
    "yarn": ">=1.22.4"
  },
  "scripts": {
    "start": "yarn && NODE_ENV=development webpack serve --mode=development",
    "build": "yarn && NODE_ENV=production webpack --mode=production",
    "bundle-analyze": "yarn && NODE_ENV=production webpack --mode=production --analyze",
    "type-check": "yarn tsc",
    "lint": "eslint './src/**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx,js,jsx}' && prettier --write './src/**/*.{ts,tsx,js,jsx}'",
    "test": "jest --config=src/jest.config.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "core-js": "^3.6.5",
    "dialog-polyfill": "^0.5.3",
    "history": "^5.0.0",
    "html2canvas": "^1.0.0-rc.5",
    "react": "^16.13.1",
    "react-color": "^2.18.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^6.0.0-beta.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-undo": "^1.0.1",
    "styled-components": "^5.1.1"
